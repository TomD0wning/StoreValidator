@model Store


<section> 
    <h3>@Model.Name</h3>
    <div>
        StoreType: @Model.StoreType
    </div>
    <div>
        <a asp-action="Details" asp-route-id="@Model.Id">More</a>
        <a asp-page="/Stores/Edit" asp-route-id="@Model.Id">Edit</a>
        <a asp-page="/Stores/Remove" asp-route-id="@Model.Id">Delete</a>
    </div>

</section>